#!hoc
/* default associativity */
func int default( \
	    int a, int b, int c, int d,      \
        int e, int f, int g, int h,      \
        int i, int j, int k, int l,      \
        int m)                           \
    return a && b && c && d || e || f || \
           g || h && i && j || k || l || m;

/* left associativity */
func int left_asoc(                          \
        int n, int o, int p, int q,          \
        int r, int s, int t, int u,          \
        int v, int w, int x, int y,          \
        int z)                               \
    return (((((((((n&&o)&&p)&&q)||r)||s)||t)\
           ||((u&&v)&&w))||x)||y)||z;

/* right associativity */
func int right_asoc(                     \
        int aa, int ab, int ac, int ad,      \
        int ae, int af, int ag, int ah,      \
        int ai, int aj, int ak, int al,      \
        int am)                           \
    return ((aa&&(ab&&(ac&&ad)))||(ae||(af||(ag \
           ||((ah&&(ai&&aj))||(ak||(al||am)))))));

