func double square_root(double x, double eps) {
    double a = x, b = 1.0;
    while (abs(a - b) > eps) {
        a = (a + b) / 2.0; /* arithmetic average */
        b = x / a;         /* geometric average (approx) */
        brkpt;
    }
    return b;
}
